{
  "name": "training",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "dev:remix": "cross-env NODE_ENV=development @remix-run/dev:remix dev",
    "blowup": "edgedb instance destroy -I \"newcore\" --force && edgedb project init",
    "client": "npx @edgedb/generate edgeql-js",
    "seed": "rm -rf dbschema/migrations  \"edgedb -d create database tmp\"\nedgedb -d tmp query \"drop database edgedb\" \"create database edgedb\" && edgedb migration create && edgedb migrate && npm run seed2",
    "seed2": "npx @edgedb/generate edgeql-js && npx @edgedb/generate interfaces && npx esbuild-runner dbschema/seed.ts",
    "seeder": " npx esbuild-runner dbschema/seed.ts",
    "gen:edgeql": "npx @edgedb/generate edgeql-js --target ts --out dbschema/codegen/edgeql/",
    "gen:queries": "npx @edgedb/generate queries --target ts",
    "gen:interfaces": "npx @edgedb/generate interfaces --file dbschema/codegen/interfaces.ts",
    "setup": "edgedb migrate && npm run seed",
    "start": "remix-serve build",
    "cleanup": "rm package-lock.json && rm -rf public/build && rm -rf  build node_modules && npm install",
    "update": "npx ncu -u && npm install",
    "query": "nodemon --watch './_querytest/queries/**/*.ts' --ext 'ts' --exec 'ts-node _querytest/index.ts'",
    "gen:query": "npx @edgedb/generate queries"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
